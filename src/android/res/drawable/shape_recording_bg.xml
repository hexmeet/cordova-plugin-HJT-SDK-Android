<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <!--填充背景色 -->
    <solid android:color="#AB000000" />
    //solid 指的是背景颜色的设置


    <!-- 设置弧度  Radius设置所有角的弧度 -->         //radius 指的就是角的弧度
